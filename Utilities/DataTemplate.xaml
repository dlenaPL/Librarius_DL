        <view:Settings />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:StatusesVM}">
        <view:Statuses />
    </DataTemplate>
    
    <DataTemplate DataType="{x:Type vm:FineStatusesVM}">
        <view:FineStatuses />
    </DataTemplate>
    
    <DataTemplate DataType="{x:Type vm:ConditionsVM}">
        <view:Conditions />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:PermissionsVM}">
        <view:Permissions />
