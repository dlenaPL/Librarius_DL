<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vm="clr-namespace:Librarius_DL.ViewModels"
                    xmlns:view="clr-namespace:Librarius_DL.Views">

    <DataTemplate DataType="{x:Type vm:HomeVM}">
        <view:Home />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:AuthorsVM}">
        <view:Authors />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:GenresVM}">
